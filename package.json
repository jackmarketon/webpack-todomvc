{
  "name": "webpack",
  "version": "0.0.1",
  "author": "srigi",
  "license": "MIT",
  "scripts": {
    "env": "env",
    "clean": "rm -rf dist/*",
    "lint": "eslint src/**/*.js src/**/*.jsx test/**/*",
    "prebuild:dev": "npm run clean && npm run lint",
    "build:dev": "webpack --debug --devtool source-map --progress",
    "build": "PROD_DEV=1 webpack",
    "server:dev": "webpack-dev-server --content-base public --debug --devtool source-map --port 8000 --progress",
    "test": "./node_modules/.bin/karma start --single-run"
  },
  "dependencies": {
    "classnames": "^1.1.4",
    "flux": "^2.0.1",
    "lodash": "^3.5.0",
    "react": "^0.13.0"
  },
  "devDependencies": {
    "babel-core": "^4.7.3",
    "babel-eslint": "^3.1.26",
    "babel-loader": "^4.1.0",
    "babel-runtime": "^4.7.3",
    "css-loader": "^0.9.1",
    "eslint": "^0.24.1",
    "file-loader": "^0.8.1",
    "jasmine-core": "^2.2.0",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.0",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "phantomjs": "^1.9.17",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0"
  }
}
